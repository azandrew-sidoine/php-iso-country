{
    "name": "drewlabs/iso-country",
    "description": "Provides International Standards Organization (ISO) country data such as iso3166, ITU-T etc..",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "iso3166",
        "ITU-T",
        "country"
    ],
    "autoload": {
        "psr-4": {
            "Drewlabs\\ISO\\Country\\": "src/"
        }
    },
    "authors": [
        {
            "name": "azandrew-sidoine",
            "email": "asmyns.platonnas29@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "require": {
        "php": "^7.0|^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": ">=6.0"
    }
}